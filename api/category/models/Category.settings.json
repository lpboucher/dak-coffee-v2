{
  "connection": "default",
  "collectionName": "categories",
  "info": {
    "name": "category",
    "description": "Product categories"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "display": {
      "type": "component",
      "repeatable": false,
      "component": "translations.name"
    },
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "string",
      "index": true
    },
    "order": {
      "type": "integer"
    },
    "coffees": {
      "collection": "coffee"
    },
    "merchandises": {
      "collection": "merchandise"
    },
    "equipment": {
      "collection": "equipment"
    }
  }
}
